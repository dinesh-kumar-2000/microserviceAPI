{
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "YourSuperSecretKeyYourSuperSecretKey@123",
    "Issuer": "MYAPP",
    "Audience": "MYAPPAudience",
    "ExpiryInMinutes": 60
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "EnableConsole": true,
    "EnableFile": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
